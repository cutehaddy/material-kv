{
    "KivyMD Toolbar Template": {
        "prefix": "md.create.toolbar",
        "body": [
             "from kivy.lang import Builder",
             "from kivymd.app import MDApp",
             "from kivy.uix.screenmanager import Screen",
             "",
             "Builder.load_string(\"\"\"",
             "<${1:Demo}>:",
             "\tMDBoxLayout:",
             "\t\torientation: \"vertical\"",
             "\t\tMDToolbar:",
             "\t\t\ttitle: \"Title\"",
             "\t\t\tmd_bg_color: [0, 1, 0, 1]",
             "\t\t\tspecific_text_color: [1, 1, 1, 1]",
             "\t\t\televation: dp(10)",
             "\t\t\tleft_action_items: [[\"menu\", lambda x: root.callback1()]]",
             "\t\t\tright_action_items: [[\"dots-vertical\", lambda x: root.callback2()]]",
             "\t\tMDBoxLayout:",
             "\"\"\")",
             "",
             "class ${1:Demo}(Screen):",
             "",
             "\tdef callback1(self):",
             "\t\tprint(\"Left Icon Pressed!\")",
             "",
             "\tdef callback2(self):",
             "\t\tprint(\"Right Icon Pressed!\")",
             "",
             "if __name__ == \"__main__\":",
             "",
            "\tclass Test(MDApp):",
            "\t\tdef build(self):",
            "\t\t\treturn ${1:Demo}()",
            "",
            "\tTest().run()"
        ]
    },
